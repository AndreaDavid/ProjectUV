                    "",
                    "var ctx = Ext.getCmp('ctn_chartjs_line');",
                    "var datasets=[];",
                    "var dt = records[0].data.datasets;",
                    "var csv=\"\";",
                    "var colores=[];",
                    "var header=\"\\n\";",
                    "header+=\"dia\";",
                    "for(var h in dt){",
                    "    colores.push({color:dt[h].borderColor});",
                    "    header+=\",\";",
                    "    header+=dt[h].label;",
                    "}",
                    "header+=\"\\n\";",
                    "csv+=header;",
                    "for(var r in records[0].data.fechas){",
                    "    var row=records[0].data.fechas[r];",
                    "    for(var data in dt){",
                    "        row+=\",\";",
                    "        row+=dt[data].cantidad[r];",
                    "    }",
                    "    row+=\"\\n\";",
                    "    csv+=row;",
                    "}",
                    "",
                    "return [csv,colores];"
                ]
